{
    "services": {
        "caddy": {
            "directory": "/app/caddy",
            "command": "caddy run --environ --config /etc/Caddyfile"
        },
        "conduit": {
            "directory": "/app",
            "command": "conduit",
            "environment": {
                "CONDUIT_CONFIG": "/app/conduit.toml"
            }
        }
    }
}