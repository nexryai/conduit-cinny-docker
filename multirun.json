{
    "services": {
        "caddy": {
            "directory": "/app/caddy",
            "command": "caddy run --environ --config /etc/Caddyfile"
        },
        "conduit": {
            "directory": "/app",
            "command": "conduit",
            "environment": {
                "CONDUIT_CONFIG": "",
                "CONDUIT_DATABASE_PATH": "/app/data",
                "CONDUIT_ADDRESS": "127.0.0.1",
                "CONDUIT_PORT": "6167"
            }
        }
    }
}
