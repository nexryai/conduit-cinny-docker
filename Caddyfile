:3000 {
    header /.well-known/matrix/* Content-Type application/json
	header /.well-known/matrix/* Access-Control-Allow-Origin *
	respond /.well-known/matrix/server `{"m.server": "{$CONDUIT_SERVER_NAME}:443"}`
    
    handle_path /_matrix/* {
        reverse_proxy  127.0.0.1:6167
    }
    
    handle {
        root * /var/cinny
        file_server
    }
}